{
  "expo": {
    "newArchEnabled": true,
      "name": "TaskWise",
  "slug": "TaskWise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TaskWiseLogo2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/TaskWiseLogo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D87E1"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/TaskWiseLogo2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
